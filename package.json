{
  "name": "project-starter-webpack",
  "version": "1.0.2",
  "description": "A test project for Min Webpack",
  "main": "index.js",
  "scripts": {
    "build": "webpack -p",
    "dev": "webpack -d",
    "watch": "webpack -d --watch",
    "test:a11y": "node tests/accessibility/compliance/pa11y.js",
    "test:visual-init": "pushd tests/visual/ && backstop init && rm backstop.json && popd",
    "test:visual-reference": "pushd tests/visual/ && ../../node_modules/backstopjs/cli/index.js reference --config=index.js && popd",
    "test:visual": "pushd tests/visual/ && ../../node_modules/backstopjs/cli/index.js test --config=index.js && popd",
    "test:visual-approve": "pushd tests/visual/ && ../../node_modules/backstopjs/cli/index.js approve --config=index.js && popd",
    "test": "jest && npm run test:a11y",
    "storybook": "start-storybook -p 6006",
    "storybook-build": "build-storybook"
  },
  "author": "Tim Wright",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@babel/register": "^7.7.4",
    "@storybook/html": "^5.3.19",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "backstopjs": "^4.4.2",
    "browserslist": "^4.8.3",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.0.0",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "file-loader": "^4.3.0",
    "husky": "^4.2.5",
    "image-webpack-loader": "^5.1.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "pa11y": "^5.3.0",
    "postcss": "^7.0.36",
    "postcss-custom-media": "^7.0.8",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "puppeteer": "^1.20.0",
    "style-loader": "^0.23.1",
    "svg-sprite-loader": "^4.1.6",
    "url-loader": "^2.3.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "webfontloader": "^1.6.28"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "maintained node versions",
    "IE 11"
  ],
  "testing": {
    "urls": {
      "local": "http://localhost:8888"
    },
    "accessibility": {
      "compliance": "WCAG2AA"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  }
}
